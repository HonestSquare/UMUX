		}
		this.setJoinPlayer = function(includedTeam, toTeam, name, id){			// 					í”Œë ˆì´ì–´ íˆ¬ì…
			if(includedTeam == toTeam) n.announce(n.cnot() + "ì¤‘ë³µëœ ëª…ë ¹ì–´ì…ë‹ˆë‹¤.", id, 'orange', 5, 1);
			else{
				switch(toTeam){  // 0: ê´€ì¤‘, 1: ë ˆë“œ, 2: ë¸”ë£¨
					case 0:	n.announce(n.notice() + 'ê´€ì¤‘ì„ìœ¼ë¡œ ì´ë™í–ˆìŠµë‹ˆë‹¤.', id, 'green', 1, 0); break;
					case 1: n.announce(n.notice() + name + 'ë‹˜ì´ ë ˆë“œíŒ€ìœ¼ë¡œ ì°¸ê°€í–ˆìŠµë‹ˆë‹¤.', null, 'green', 4, 0); break;
					case 2: n.announce(n.notice() + name + 'ë‹˜ì´ ë¸”ë£¨íŒ€ìœ¼ë¡œ ì°¸ê°€í–ˆìŠµë‹ˆë‹¤.', null, 'green', 4, 0); break;
				}
				room.setPlayerTeam(id, toTeam);

//-----------------------------------------------------------------------
var room = HBInit({ 
	roomName: "UMUXì˜ ë°©",
	maxPlayers: 15,         // ìµœëŒ€ ì¸ì›
	playerName : " ",       // ë°©ì¥ ì´ë¦„(ê·¸ëŒ€ë¡œ ë†”ë‘ëŠ” ê±¸ ê¶Œì¥)
	public : true,          // ê³µê°œë°© ì„¤ì •
	noPlayer: false         // ë°©ì¥ ì—†ì• ê¸°(ê·¸ëŒ€ë¡œ ë†”ë‘ëŠ” ê±¸ ê¶Œì¥)
});
room.setCustomStadium(map[0]);		// ê¸°ë³¸ ì„¤ì •ëœ ë‚´ì¥ë§µ
room.setScoreLimit(5);                  // ê¸°ë³¸ ì ìˆ˜ ì œí•œ
room.setTimeLimit(5);                   // ê¸°ë³¸ ì‹œê°„ ì œí•œ
//----------------------------------------------------------------------------------------------------
var date = new Date();
var Roomversion = 'v1.00';	        // ë´‡ë°© ë²„ì „
var UXversion  = '1.0.0';               // UMUX ë²„ì „
var dev = false;			// ê°œë°œ ë²„ì „ ì„¤ì •

class GameManager{													// ê²Œì„ ë§¤ë‹ˆì € í´ë˜ìŠ¤
	constructor(){
		this.gamestats;											// 0: ì •ì§€, 1: ì‹œì‘, 2: ê²Œì„ ì¤‘

		this.onGameStart = function(){							// ê²Œì„ ì‹œì‘
			game.gamestats = 1;
			cout(true, 'ê²Œì„ ì‹œì‘.');
			return false;
		}
		this.onGameTick = function(){							// ê²Œì„ ë„ì¤‘
			game.gamestats = 2;
			return false;
		}
		this.onGameStop = function(){							// ê²Œì„ ì¢…ë£Œ
			game.gamestats = 0;
			cout(true, 'ê²Œì„ ì¢…ë£Œ');
		}
		this.onGamePause = function(byPlayer){ 					// ê²Œì„ ì¤‘ë‹¨
			if(byPlayer.id) cout(true, byPlayer.name + '(ì´)ê°€ ê²Œì„ì„ ì¼ì‹œ ì¤‘ë‹¨í•¨.');
			else cout(true, 'ê²Œì„ ì¼ì‹œ ì¤‘ë‹¨.');		// ëª…ë ¹ì–´ ì…ë ¥ìœ¼ë¡œ ì‹¤í–‰ë˜ë©´ í”Œë ˆì´ì–´ ì´ë¦„ ê°€ë¦¼
			return false;
		}	
		this.onGameUnpause 	= function(byPlayer){				// ê²Œì„ ì¬ê°œ
			if(byPlayer.id) cout(true, byPlayer.name + '(ì´)ê°€ ê²Œì„ì„ ì¬ê°œí•¨.');
			else cout(true, 'ê²Œì„ ì¬ê°œ.');			// ëª…ë ¹ì–´ ì…ë ¥ìœ¼ë¡œ ì‹¤í–‰ë˜ë©´ í”Œë ˆì´ì–´ ì´ë¦„ ê°€ë¦¼
			return false;
		}
		this.onPlayerJoin = function(player) {											// í”Œë ˆì´ì–´ ì…ì¥
			p.cntPlayers++;
			p.getNumbersFun();																// í• ë‹¹
			room.sendChat("This room only supports Korean language. :", player.id);		
			n.devCheck(player);																// ê°œë°œì ë²„ì „ ì²´í¬
			chat.alltoChat(player, n.notice() + player.name + 'ë‹˜ì´ ì…ì¥í–ˆìŠµë‹ˆë‹¤.', true);		// ì•ˆë‚´ ë©”ì‹œì§€
			// ì‚¬ì¹­ ë° ë¸”ë™ë¦¬ìŠ¤íŠ¸ íƒì§€
			if(admin.filterPlayer(player)){ 
				n.announce(n.notice() + player.name + 'ë‹˜ì€ ê´€ì‹¬ ëŒ€ìƒì…ë‹ˆë‹¤.', player.id, 'green', 5, 3);
				cout(true, player.name + '(ì´)ê°€ ì…ì¥í•¨. (ë¸”ë™ë¦¬ìŠ¤íŠ¸)');
			}
			else{ 
				n.announce(n.notice() + 'ì•ˆë…•í•˜ì„¸ìš”, ' + player.name + 'ë‹˜! (ìœ ìš©í•  ë„ì›€ë§: !help, !join)', player.id, null, 1, 1);
				cout(true, player.name + '(ì´)ê°€ ì…ì¥í•¨.');
			}
			admin.updateAdmins(player);
			if(p.cntPlayers == 1){ 
				room.startGame();
				com.setJoinPlayer(player.team, 1, player.name, player.id);					// ì‚¬ëŒì´ í•œ ëª…ì´ë©´ ìë™ ì‹œì‘ ë° íˆ¬ì…
			}
			return false;
		}
		this.onPlayerLeave = function(player) {											// í”Œë ˆì´ì–´ í‡´ì¥
			if(!admin.kicked){													// ê°•ì œ í‡´ì¥ í™•ì¸
				cout(true, player.name + '(ì´)ê°€ í‡´ì¥í•¨.');
				n.announce(n.notice() + player.name + 'ë‹˜ì´ ë– ë‚¬ìŠµë‹ˆë‹¤.', null, 'green', 5, 1);
			}
			else admin.kicked = false;
			admin.cntSubAdmins--;
			admin.subAdmin[player.id - 1] = false;
			admin.updateAdmins(player);										// ê¶Œí•œ ì—¬ë¶€ í™•ì¸
			// ìœ ì € ëª©ë¡ ì •ë¦¬
			p.updatePlayers(player);  
			p.getNumbersFun();
			p.cntPlayers--;
			if(!p.cntPlayers) room.stopGame();							// ì‚¬ëŒì´ ì—†ìœ¼ë©´ ìë™ ì¢…ë£Œ
			return false;
		}
		this.onStadiumChange = function(mapName){				// ë§µ êµì²´
			if(p.cntPlayers){
				if(Math.floor(Math.random()*6) == 0) n.announce(n.msgCommand('ë§µ ì €ì¥ ') + '/store', null, 'green', 1, 3);
				cout(true, 'ë§µ êµì²´: ' + mapName);
			}
			else cout(true, 'ì„œë²„ ì˜¤í”ˆ');
			return false;
		}
		this.onPositionsReset = function(){		 				// ì¢Œí‘œ ì´ˆê¸°í™”
			return false; 
		}
		this.onPlayerBallKick = function(){ 					// ê³µ ì°¼ì„ ë•Œ
			return false; 
		}
		this.onTeamGoal = function(team){ 						// ê³¨ ë¨¹í ë•Œ
			if(team == 1){ 
				n.announce(n.notice() + 'ë ˆë“œíŒ€ì´ ë“ì í–ˆìŠµë‹ˆë‹¤.');
				cout(true, 'ë ˆë“œíŒ€ ë“ì ');
			}
			if(team == 2){ 
				n.announce(n.notice() + 'ë¸”ë£¨íŒ€ì´ ë“ì í–ˆìŠµë‹ˆë‹¤.');
				cout(true, 'ë¸”ë£¨íŒ€ ë“ì ');
			}
			return false; 
		}
		this.onTeamVictory = function(scores){ 					// íŒ€ ìŠ¹ë¦¬
			return false; 
		}
	}
}
class Administration{												// ê´€ë¦¬ í´ë˜ìŠ¤
	constructor() {
		this.cntAdmins = 0;									// ê´€ë¦¬ì ì¸ì›
		this.cntSubAdmins = 0;								// ë³´ì¡° ê´€ë¦¬ì ì¸ì›
		this.kicked = false;								// ê°•ì œ í‡´ì¥ ì—¬ë¶€
		this.dynamicAdmin = true	 						// ê¶Œí•œ ë™ì  í• ë‹¹
		this.subAdmin = new Array(15);						// ë³´ì¡° ê¶Œí•œ(ë¶€ ì–´ë“œë¯¼)
		this.lockChat = true;								// ì±„íŒ… ì ê¸ˆ
		this.mutedList = [];								// ì±„ê¸ˆì ëª©ë¡

		this.onPlayerAdminChange = function(player){		//							 	í”Œë ˆì´ì–´ ê¶Œí•œ íšë“&ë°•íƒˆ
			if(admin.filterPlayer(player)){ 							// ë¸”ë™ë¦¬ìŠ¤íŠ¸ ê¶Œí•œ ë¶€ì—¬ ë°°ì œ ëŒ€ìƒ
				room.setPlayerAdmin(player.id, false);
				admin.subAdmin[player.id - 1] = false;
				p.updateMarks(player);
			}
			if(!admin.checkAdmins) admin.getSubAdmin(player);		// ê´€ë¦¬ìê°€ ì—†ìœ¼ë©´ ë³´ì¡° ê¶Œí•œ ë¶€ì—¬
			else{
				if(player.admin){									// í”Œë ˆì´ì–´ê°€ ê´€ë¦¬ìë©´ ë³´ì¡° ê¶Œí•œ ì§€ìš°ê¸°
					admin.subAdmin[player.id - 1] = false;
					admin.cntSubAdmins--;
				}
				else admin.getSubAdmin(player);
			}
			return false;
		}
		this.onPlayerKicked = function(kickedPlayer, reason, ban, byPlayer){			//	í”Œë ˆì´ì–´ ê°•ì œ í‡´ì¥
			admin.kicked = true;
			if(reason) cout(true, byPlayer.name + '(ì´)ê°€ ' + kickedPlayer.name + '(ì„)ì„ ê°•ì œ í‡´ì¥í•¨. (ì‚¬ìœ : ' + reason + ')');
			else cout(true, byPlayer.name + '(ì´)ê°€ ' + kickedPlayer.name + '(ì„)ì„ ê°•ì œ í‡´ì¥í•¨.');
			return admin.kicked;
		}
		this.adminHelp = function(player){					// !adminhelp				|	ê´€ë¦¬ ëª…ë ¹ì–´
			if (player.admin || admin.subAdmin[player.id -1])
				n.announce(n.msgCommand('ê´€ë¦¬ ') 
				+ 'p | !p | !clearbans | !time ìˆ«ì | !set_pw | !score ìˆ«ì | !rr | !load ìˆ«ì | !lock | !ë„ ' + com.recommendCom('ê´€ë ¨', '!ahc'), player.id, null, 4, 3);
			else n.acess(player);
			return false;  // ëª…ë ¹ì–´ í”ì ì„ ë‚¨ê¸°ì§€ ì•ŠìŒ
		}
		this.adminComHelp = function(player){				// !ahc						|	ê´€ë¦¬ ëª…ë ¹ì–´ ë„ì›€ë§
			if (player.admin || admin.subAdmin[player.id -1]){
				n.announce(n.msgCommand('ê´€ë¦¬ ') + 'p/!p(ê²Œì„ ì¼ì‹œì •ì§€/ì‹œì‘) | !clearbans(ë°´ ë¦¬ìŠ¤íŠ¸ ì´ˆê¸°í™”)', player.id, null, 4, 3);
				n.announce(n.msgCommand('ê´€ë¦¬ ') + '!time(ì‹œê°„ ì„¤ì •) | !score(ì ìˆ˜ ì„¤ì •) | !rr(ê²Œì„ ì¬ì‹œì‘)', player.id, null, 4, 3);
				n.announce(n.msgCommand('ê´€ë¦¬ ') + '!load ID: ë§µ ë¡œë“œ', player.id, null, 4, 3);
				n.announce(n.msgCommand('ê´€ë¦¬ ') + '!lock(íŒ€ ì´ë™ ê¸ˆì§€/í—ˆìš©) | !ë„(ë„ë°°ë°©ì§€ë¬¸ì ì¶œë ¥)', player.id, null, 4, 3);
			}
			else n.acess(player);
			return false;  // ëª…ë ¹ì–´ í”ì ì„ ë‚¨ê¸°ì§€ ì•ŠìŒ
		}
		this.updateAdmins = function(player) {				// 								ì–´ë“œë¯¼ ì—†ìœ¼ë©´ ê¶Œí•œ ë¶€ì—¬
			var players = room.getPlayerList().filter((player) => player.id != 0 );
			// ì•„ë¬´ë„ ì•ˆ ë‚˜ê°€ë©´ ë™ì‘ ì—†ìŒ
			if ( players.length == 0 ) return; 
			  // ì–´ë“œë¯¼ ë‚˜ê°€ë©´ ì•„ë¬´ ë™ì‘ ì—†ìŒ
			  if ( players.find((player) => player.admin) != null ) return; 
			  admin.secAdmin = true;
			  // ì–´ë“œë¯¼ ì—†ìœ¼ë©´ ë¶€ì—¬
			  if(admin.dynamicAdmin){
					if(admin.filterPlayer(player)) return admin.getSubAdmin(player);
			  		else{
						  room.setPlayerAdmin(player.id, true);
						  n.announce(n.notice() + player.name + 'ë‹˜ì´ ê¶Œí•œì„ íšë“í–ˆìŠµë‹ˆë‹¤. ' + com.recommendCom('ê´€ë ¨', '!adminhelp'), null, 'pink', 5, 2);
						  cout(true, player.name + '(ì´)ê°€ ê¶Œí•œì„ ì–»ìŒ.');
				  }
			  }
		}
		this.getAdminstats = function(player){				// 								ì–´ë“œë¯¼ ê¶Œí•œ í™•ì¸
			if(player.admin) return true;
			else return false;
		}
		this.setAdmin = function(player){					// 								ê¶Œí•œ ë™ì  í• ë‹¹ ì˜µì…˜
			if(player.admin || admin.subAdmin[player.id - 1]){
				if(!admin.dynamicAdmin){
					admin.dynamicAdmin = true;
					n.announce(n.unlocked() + 'ë™ì  í• ë‹¹ì´ í™œì„±í™”ë˜ì—ˆìŠµë‹ˆë‹¤.', player.id, 'red', 2, 1);
					cout(true, player.name + '(ì´)ê°€ ë™ì  í• ë‹¹ì„ í™œìƒí™”í•¨.');
				}
				else{
					admin.dynamicAdmin = false;
					n.announce(n.locked() + 'ë™ì  í• ë‹¹ì´ ë¹„í™œì„±í™”ë˜ì—ˆìŠµë‹ˆë‹¤.', player.id, 'red', 2, 1);
					cout(true, player.name + '(ì´)ê°€ ë™ì  í• ë‹¹ì„ ë¹„í™œìƒí™”í•¨.');
				}
				return false;
			}
			else return n.acess(player);
		}
		this.setScore = function(player, msg){				// !score n 				|	ì ìˆ˜ ë³€ê²½ ëª…ë ¹ì–´(onlyadmin)
			if (player.admin || admin.subAdmin[player.id - 1]){
				if(!game.gamestats){
					if((msg.substr(6)) >= 0){
						if((msg.substr(6)) < 15){
							room.setScoreLimit(msg.substr(6));
							n.announce(n.notice() + 'ì œí•œ ì ìˆ˜ê°€ ' + msg.substr(6) + 'ì ìœ¼ë¡œ ë³€ê²½ë˜ì—ˆìŠµë‹ˆë‹¤.', null, 'yellow', 4, 1);
							cout(true, player.name + '(ì´)ê°€ ì œí•œ ì ìˆ˜ë¥¼ ' + msg.substr(6) + 'ì ìœ¼ë¡œ ë³€ê²½í•¨.');
						}
					}
				}
				else n.announce(n.cnot() + 'íŒì´ ì™„ì „íˆ ëë‚œ ì´í›„ì— ë‹¤ì‹œ ì‹œë„í•´ ë³´ì„¸ìš”.', player.id, 'orange', 5, 3);
			}
			else n.acess(player);
			return false;  // ëª…ë ¹ì–´ í”ì ì„ ë‚¨ê¸°ì§€ ì•ŠìŒ
		}
		this.setTime = function(player, msg){				// !time n					|	ì‹œê°„ ë³€ê²½ ëª…ë ¹ì–´(onlyadmin)
			if (player.admin || admin.subAdmin[player.id - 1]){
				if(!game.gamestats){
					if((msg.substr(6)) >= 0){
						if((msg.substr(6)) < 15){
							room.setTimeLimit(msg.substr(6));
							n.announce(n.notice() + 'ì œí•œ ì‹œê°„ì´ ' + msg.substr(6) + 'ë¶„ìœ¼ë¡œ ë³€ê²½ë˜ì—ˆìŠµë‹ˆë‹¤.', null, 'yellow', 4, 1);
							cout(true, player.name + '(ì´)ê°€ ì œí•œ ì‹œê°„ì„ ' + msg.substr(6) + 'ë¶„ìœ¼ë¡œ ë³€ê²½í•¨.');
						}
					}
				}
				else n.announce(n.cnot() + 'íŒì´ ì™„ì „íˆ ëë‚œ ì´í›„ì— ë‹¤ì‹œ ì‹œë„í•´ ë³´ì„¸ìš”.', player.id, 'orange', 5, 3);
			}
			else n.acess(player);
			return false;  // ëª…ë ¹ì–´ í”ì ì„ ë‚¨ê¸°ì§€ ì•ŠìŒ
		}
		this.setTeamsLock = function(player){				// !lock					|	 íŒ€ ì´ë™ ê¸ˆì§€/í—ˆìš© ëª…ë ¹ì–´
			if(player.admin || admin.subAdmin[player.id - 1]){
				if(admin.lockChat){		// ì°¸ì´ë©´ ê¸ˆì§€, ê±°ì§“ì´ë©´ í—ˆìš©
					room.setTeamsLock(admin.lockChat);
					n.announce(n.locked() + 'íŒ€ ììœ¨ ì´ë™ì´ ê¸ˆì§€ë˜ì—ˆìŠµë‹ˆë‹¤.', null, 'pink', 2, 2);
					admin.lockChat = false;
					cout(true, player.name + '(ì´)ê°€ íŒ€ ì´ë™ì„ ê¸ˆì§€í•¨.');
				}
				else{
					room.setTeamsLock(admin.lockChat);
					n.announce(n.unlocked() + 'íŒ€ ììœ¨ ì´ë™ì´ í—ˆìš©ë˜ì—ˆìŠµë‹ˆë‹¤.', null, 'pink', 2, 2);
					admin.lockChat = true;
					cout(true, player.name + '(ì´)ê°€ íŒ€ ì´ë™ì„ í—ˆìš©í•¨.');
				}
			}
			else n.acess(player);
			return false;
		}
		this.setReset = function(player){					// !rr						|	ê²Œì„ ì¬ì‹œì‘
			if(player.admin || admin.subAdmin[player.id - 1]){
				room.stopGame();
				room.startGame();
				cout(true, 'ë‹¤ì‹œ ì‹œì‘.');
			}
			return false;  // ëª…ë ¹ì–´ í”ì ì„ ë‚¨ê¸°ì§€ ì•ŠìŒ
		}
		this.setPassword = function(player, pass){			// !set_pw string			|	ë¹„ë²ˆ ì„¤ì •
			if(player.admin || admin.subAdmin[player.id - 1]){
				if(pass.length <= 10) n.announce(n.locked() + 'ë¹„ë°€ë²ˆí˜¸ê°€ ë„ˆë¬´ ì§§ìŠµë‹ˆë‹¤.', player.id, 'yellow', 0, 0);
				else if(pass.length >= 28) n.announce(n.locked() + 'ë¹„ë°€ë²ˆí˜¸ê°€ ë„ˆë¬´ ê¸¸ì–´ì„œ ì„¤ì •í•  ìˆ˜ ì—†ìŠµë‹ˆë‹¤.', player.id, 'yellow', 0, 0);
				else{ 
					n.announce(n.locked() + 'ë¹„ë°€ë²ˆí˜¸ê°€ ì„¤ì •ë˜ì—ˆìŠµë‹ˆë‹¤.', null, 'pink', 2, 1);
					return room.setPassword(pass.substr(8, 20));
				}
			}
			else n.acess(player);
			return false;
		}
		this.missPassword = function(player){ 				// 								ì–´ë“œë¯¼ ì „ìš© ëª…ë ¹ì–´ ì˜¤íƒ€ ë°©ì§€
			cout(true, player.name + '(ì´)ê°€ ê´€ë¦¬ì ë¡œê·¸ì¸ì„ ì‹œë„í•¨. (ì‹¤íŒ¨)');
			return false; 
		}
		this.getAdmin = function(player, _msg){				// 								ê¶Œí•œ íšë“
			if (!player.admin){
				if(!admin.filterPlayer(player)){
					room.setPlayerAdmin(player.id, true);
					admin.subAdmin[player.id - 1] = false;
					n.announce(n.notice() + player.name + 'ë‹˜ì´ ê¶Œí•œì„ íšë“í–ˆìŠµë‹ˆë‹¤. ' + com.recommendCom('ê´€ë ¨', '!adminhelp'), null, 'pink', 5,2);
					cout(true, player.name + '(ì´)ê°€ ê¶Œí•œì„ ì–»ìŒ.');
				}
			}
			return false; // ëª…ë ¹ì–´ ì…ë ¥í•œ í”ì ì´ ë‚¨ì§€ ì•ŠìŒ
		}
		this.getSubAdmin = function(player){				// 								ë³´ì¡° ê¶Œí•œ íšë“
			if (!player.admin){
				admin.subAdmin[player.id - 1] = true;	// ë³´ì¡° ê¶Œí•œ ì„¤ì •
				admin.cntSubAdmins++;					// ë³´ì¡° ê´€ë¦¬ì ê°±ì‹ 
				p.updateMarks();						// ë§ˆí¬ ê°±ì‹ 
				n.announce(n.notice() + player.name + 'ë‹˜ì´ ë³´ì¡° ê¶Œí•œì„ íšë“í–ˆìŠµë‹ˆë‹¤. ' + com.recommendCom('ê´€ë ¨', '!adminhelp'), null, 'pink', 2, 2);
				cout(true, player.name + '(ì´)ê°€ ë³´ì¡° ê¶Œí•œì„ ì–»ìŒ.');
			}
			return false; // ëª…ë ¹ì–´ ì…ë ¥í•œ í”ì ì´ ë‚¨ì§€ ì•ŠìŒ
		}
		this.checkAdmins = function(){						// 								ê´€ë¦¬ì ì¸ì› ì²´í¬
			var members = room.getPlayerList();
			for(var i = 0; i < p.cntPlayers; i++){
				if(admin.getAdminstats(members[i])) admin.cntAdmins++;
			}
			return admin.cntAdmins;
		}
		this.checkSubAdmins = function(){					// 								ë³´ì¡° ê´€ë¦¬ì ì¸ì› ì²´í¬
			var members = room.getPlayerList();
			for(var i = 0; i < p.cntPlayers; i++){
				if(admin.subAdmin[members[i] - 1]) admin.cntSubAdmins++;
			}
			return admin.cntSubAdmins;
		}
		this.clearBans = function(player){					// !clearbans				|	ê°•ì œ í‡´ì¥ ëª©ë¡ ì´ˆê¸°í™” ëª…ë ¹ì–´
			if (player.admin || admin.subAdmin[player.id - 1]){
				room.clearBans();
				n.announce(n.notice() + 'Ban ì²˜ë¦¬ ë‚´ì—­ì´ ì´ˆê¸°í™”ë˜ì—ˆìŠµë‹ˆë‹¤.', null, 'pink', 2, 2);
				cout(true, player.name + '(ì´)ê°€ ì˜êµ¬ í‡´ì¥ ëª©ë¡ì„ ëª¨ë‘ ì§€ì›€.');
			}
			else n.acess(player);
			return false;  // ëª…ë ¹ì–´ í”ì ì„ ë‚¨ê¸°ì§€ ì•ŠìŒ
		}
		this.showPassword = function(player){				// !show_pw					| 	ë¹„ë²ˆ ê³µê°œ
			if(player.admin || admin.subAdmin[player.id - 1]) n.announce(n.notice() + 'í˜„ì¬ ë¹„ë°€ë²ˆí˜¸ëŠ” ' + room.password + 'ì…ë‹ˆë‹¤.', player.id, 'green', 0, 0);
			else n.acess(player);
			return false;
		}
		this.releasePassword = function(player){			// !clear_pw				|	ë¹„ë²ˆ í•´ì œ
			if(player.admin || admin.subAdmin[player.id - 1]){
				n.announce(n.unlocked() + 'ë¹„ë°€ë²ˆí˜¸ê°€ í•´ì œë˜ì—ˆìŠµë‹ˆë‹¤.', null, 'pink', 2, 1);
				return room.setPassword();
			}
			else n.acess(player);
			return false;
		}
		this.filterPlayer = function(player){				//								ì‚¬ì¹­ ë° ì¤‘ë³µ í•„í„°(onlyadmin)
			let i = 0;
			p.members = room.getPlayerList();

			if(p.cntPlayers > 1)											// ì‚¬ì¹­ ê°ì§€
			{
				for (let i = 1; i <= p.cntPlayers; i++){
					var checkname = p.members[i].name;
					if((p.members[i].id != player.id)&&(checkname == player.name))
						return admin.doKick(player, "ì‚¬ì¹­ ë° ì¤‘ë³µ ì ‘ì† ê°ì§€ ì‹œìŠ¤í…œìœ¼ë¡œ ì¸í•´ í‡´ì¥ë˜ì—ˆìŠµë‹ˆë‹¤.");
				}
			}
			while(i < p.blacklist.length){									// ë¸”ë™ ë¦¬ìŠ¤íŠ¸ ê°ì§€
				// í¬í•¨ë˜ë©´ í•„í„° ë°˜í™˜ | í¬í•¨ë˜ì§€ ì•Šìœ¼ë©´ i ì¦ê°€
				if(player.name.search(p.blacklist[i]) != -1) return true;
				else i++;
			}
			return false;
		}
		this.doKick = function(player, msg){				//								ê°•ì œ í‡´ì¥ ì²˜ë¦¬
			room.kickPlayer(player.id, n.cnot() + msg, false);
			return false;
		}
		this.getMute = function(player, msg){				// !mute ID					|	ì±„ê¸ˆ ì„¤ì •
			p.members = room.getPlayerList();

			if(player.admin || admin.subAdmin[player.id - 1]){
				if((msg.substr(6)> 0)&&(msg.substr(6) <= p.cntPlayers)){
					if(msg.substr(6) == p.members[msg.substr(6)].id) n.announce(n.cnot() + 'ìì‹ ì˜ ì±„íŒ…ì„ ê¸ˆì§€í•  ìˆ˜ ì—†ìŠµë‹ˆë‹¤.', player.id, 'orange', 5, 3);
					else{
						admin.mutedList.push(p.members[msg.substr(6)].name);
						chat.alltoChat(p.members[msg.substr(6)], n.locked() + 'íŠ¹ì • í”Œë ˆì´ì–´ì˜ ì±„íŒ…ì´ ê¸ˆì§€ë˜ì—ˆìŠµë‹ˆë‹¤.', 5);
						n.announce(n.locked() + 'ì±„íŒ… ê¸ˆì§€ë˜ì—ˆìŠµë‹ˆë‹¤.', msg.substr(6), 'green', 5, 3);
					}
				}
				else n.announce(n.cnot() + 'ì˜ëª»ëœ ëŒ€ìƒì…ë‹ˆë‹¤.', player.id, 'orange', 5, 3);
			}
			else if(!player.admin || !admin.subAdmin[player.id - 1]) n.acess(player);
			return false;
		}
		this.releaseMute = function(player, msg){			// !unmute ID				|	ì±„ê¸ˆ í’€ê¸°
			p.members = room.getPlayerList();

			if(player.admin || admin.subAdmin[player.id - 1]){
				if((msg.substr(8) > 0)&&(msg.substr(8) <= p.cntPlayers)){
					if(msg.substr(8) == p.members[msg.substr(8)].id) n.announce(n.cnot() + 'ì˜ëª»ëœ ëŒ€ìƒì…ë‹ˆë‹¤.', player.id, 'orange', 5, 3);
					else{
						let pos = admin.mutedList.indexOf(id.substr(9));
						admin.mutedList.splice(pos, 1);
						chat.alltoChat(p.members[msg.substr(8)], n.unlocked() + 'íŠ¹ì • í”Œë ˆì´ì–´ì˜ ì±„íŒ…ì´ í—ˆìš©ë˜ì—ˆìŠµë‹ˆë‹¤.', 5);
						cout(true, p.members[msg.substr(8)].name + '(ì´)ì˜ ì±„ê¸ˆì´ í•´ì œë¨.');
					}
				}
				else if(!msg.substr(8)){ 
					admin.mutedList = null;
					n.announce(p.members[msg.substr(8)], n.unlocked() + 'íŠ¹ì • í”Œë ˆì´ì–´ì˜ ì±„íŒ…ì´ í—ˆìš©ë˜ì—ˆìŠµë‹ˆë‹¤.', null, 'green', 5);
					cout(true, 'ëª¨ë“  ì±„ê¸ˆìì˜ ì±„ê¸ˆì´ í•´ì œë¨.');
				}
				else n.announce(n.cnot() + 'ì˜ëª»ëœ ëŒ€ìƒì…ë‹ˆë‹¤.', player.id, 'orange', 2, 3);
			}
			else if(!player.admin || !admin.subAdmin[player.id - 1]) n.acess(player);
			return false;
		}
		this.putPause = function(player){				// p						|	ì¼ì‹œ ì •ì§€(onlyadmin)
			if(player.admin || admin.subAdmin[player.id - 1]){
				room.pauseGame(true);
				return false;  
			}
		}
		this.unPause = function(player){ 				// !p						|	ê³„ì†(onlyadmin)
			if(player.admin || admin.subAdmin[player.id - 1]) room.pauseGame(false);
			else n.acess(player);
			return false;
		}
	}
}
class Notice {														// ì•Œë¦¼ í´ë˜ìŠ¤
	constructor() {
		this.devCheck = function(player){		// ê°œë°œ ë²„ì „ ì•ˆë‚´ ë©”ì„¸ì§€
			if(dev) return n.announce(n.cnot() + 'ê°œë°œ ì „ìš© í…ŒìŠ¤íŠ¸ë°©ì´ë¯€ë¡œ ì›í™œí•œ í”Œë ˆì´ê°€ ì–´ë µìŠµë‹ˆë‹¤.', player.id, 'orange', 5, 2);
		}
		this.info = function () {				// ìƒì„¸ ì •ë³´
			return "ğŸ“¡ìƒì„¸ì •ë³´ğŸ“¡ : ";
		};
		this.help = function () {				// ë„ì›€ë§
			return "ğŸ’¬ë„ì›€ë§ğŸ—¨ : ";
		};
		this.infoplayer = function () {			// í”Œë ˆì´ì–´ ì •ë³´
			return "ğŸ”í”Œë ˆì´ì–´ ì •ë³´ğŸ” : ";
		};
		this.notice = function () {				// ì•Œë¦¼
			return "ğŸ›ì•Œë¦¼ğŸ› : ";
		};
		this.cnot = function () {				// ì£¼ì˜
			return "â›”ì£¼ì˜â›” : ";
		};
		this.locked = function () {				// ë¹„í™œì„±í™”
			return "ğŸ”’ì ê¸ˆğŸ”’ : ";
		};
		this.unlocked = function () {			// í™œì„±í™”
			return "ğŸ”“í•´ì œğŸ”“ : ";
		};
		this.acess = function(player){			// ê¶Œí•œ ì—†ìŒ
		n.announce(n.locked() + 'ê¶Œí•œì´ ì—†ìŠµë‹ˆë‹¤.', player.id, 'yellow', 0, 0);
		return false;
		}
		this.msgCommand = function(msg){ 		// ì•Œë¦¼ ë©”ì„¸ì§€
			msg = "ğŸ”" + msg + "ëª…ë ¹ì–´ğŸ” : ";
			return msg;
		}
		this.ruleCommand = function(msg){ 		// ê·œì¹™
			msg = "ğŸ•¹" + msg + "ê·œì¹™ğŸ® : ";
			return msg;
		}										// ì•Œë¦¼ ë©”ì‹œì§€
		this.announce = function(msg, target, color, style, sound){ 
			room.sendAnnouncement(msg, target, n.getColor(color), n.getStyle(style), sound);
			return false;
		}
		this.getColor = function(color){
			switch(color){
				case 'um': color = "6CB858"; break;				// UM
				case 'red': color = "FF0000"; break;			// ë¹¨ê°•
				case 'orange': color = "FF5E00"; break;			// ì£¼í™©
				case 'yellow': color = "FFE400"; break;			// ë…¸ë‘
				case 'green': color = "8ED2AB"; break;			// ì´ˆë¡
				case 'sky': color = "00D8FF"; break;			// í•˜ëŠ˜
				case 'blue': color = "0000FF"; break;			// íŒŒë‘
				case 'purple': color = "5F00FF"; break;			// ë³´ë¼
				case 'pink': color = "ff86cf"; break;			// í•‘í¬
				case 'black': color = "000000"; break;			// ê²€ì •
			}
			if(color) color = ("0x" + color);
			else color = "0xFFFFFF";
			return color;
		}
		this.getStyle = function(style){
			switch(style){
				case 1: style = "nomarl"; break;		// ê¸°ë³¸
				case 2: style = "bold"; break;			// ë³¼ë“œì²´
				case 3: style = "italic"; break;		// ì´íƒˆë¦­
				case 4: style = "small"; break;			// ì‘ê²Œ
				case 5: style = "small-bold"; break;	// ì‘ì€ ë³¼ë“œì²´
				case 6: style = "small-italic"; break;	// ì‘ì€ ì´íƒˆë¦­
			}
			return style;
		}
	}
}
class ChatSystem {													// ì±„íŒ… ì‹œìŠ¤í…œ í´ë˜ìŠ¤
	constructor(){
		this.face = [														// 						ì´ëª¨í‹°ì½˜
			'ğŸ¤”', 
			'ğŸ˜›', 'ğŸ˜', 'ğŸ˜…', 'ğŸ˜‚', 'ğŸ¤£', 
			'ğŸ˜', 'ğŸ˜', 'ğŸ˜¥', 'ğŸ˜°', 'ğŸ™„', 
			'ğŸ˜´', 'ğŸ¥¶', 'ğŸ˜±', 'ğŸ¥µ'
		]
		this.getFace = function(emoji, name){ return (emoji + name + emoji) }
		this.onPlayerChat = function(player, msg) {						//							ì±„íŒ… ì‹œìŠ¤í…œ
			let i = 0;
			let spacePos = msg.search(" ");
			let command = msg.substr(0, spacePos !== -1 ? spacePos : msg.length);

			while(i < admin.mutedList.length){
				// í¬í•¨ë˜ë©´ í•„í„° ë°˜í™˜ | í¬í•¨ë˜ì§€ ì•Šìœ¼ë©´ i ì¦ê°€
				if(player.name.search(admin.mutedList[i]) != -1) return n.announce(n.locked() + 'ì ì‹œë™ì•ˆ ì±„íŒ…ì´ ë¶ˆê°€í•©ë‹ˆë‹¤.', player.id, 'green', 5, 3);
				else i++;
			}
			if (commands.hasOwnProperty(command)) return commands[command](player, msg);	//	ëª…ë ¹ì–´
			else chat.send(player, msg, true);												//	ì¼ë°˜ ì±„íŒ…
			return false;
		}
		this.FfilterFun = function(player){
			admin.doKick(player);		// í‡´ì¥ ì²˜ë¦¬
		}
		this.send = function(player, msg, chatPublic){					//							ì¼ë°˜ ì±„íŒ…
			var fmsg = (" ì „ì²´ : [" + p.getIdofPlayer(player) + "]");
			var lmsg = (player.name + ": " + msg);

			if(chatPublic) p.getMarks(true, player, fmsg, lmsg);
			else p.getMarks(false, player, fmsg, lmsg);
			return false;
		}
		this.alltoChat = function(player, msg, type){					//							ë‚˜ ë¹¼ê³  ë‹¤ ë³´ë‚´ê¸°
			p.members = room.getPlayerList();
			for(var i = 0; i<= p.cntPlayers; i++){
				if(p.members[i].id != player.id){
					if(type) n.announce(msg, p.members[i].id, 'yellow', 5, 1);	// ê³µì§€ë¡œ ì¶œë ¥
					else room.sendChat(msg, p.members[i].id);					// ì¼ë°˜ ì±„íŒ…ìœ¼ë¡œ ì¶œë ¥
				}
			}
			return false;
		}
		this.whisperChat = function(player, msg)						// !e ID				|	ê·“ì†ë§ ëª…ë ¹ì–´
		{
			p.members = room.getPlayerList();
			for(var i = 0; i <= p.cntPlayers; i++){
				if(p.members[i] && (i == msg.substr(3, 2))){
					if(p.members[i].id == player.id) 		// ë™ì¼ ì¸ë¬¼ì¸ ê²½ìš°
					n.announce(n.cnot() + 'ìê¸° ìì‹ ì—ê²Œ ê·“ì†ë§ì„ ë³´ë‚¼ ìˆ˜ ì—†ìŠµë‹ˆë‹¤.', player.id, 'orange', 2, 3);
					else if(!p.members[i].id){				// idê°€ 0ì´ë©´ ì½˜ì†”ì°½ìœ¼ë¡œ ì „ë‹¬
						room.sendChat("ì™¸ë¶€ : [" + p.getIdofPlayer(player) + "]" + p.checkMarks(player) + player.name + "â†’ [0]ğŸŒê´€ë¦¬ì: " + msg.substr(5, 50), player.id);
						cout(false, "[" + p.getIdofPlayer(player) + "]" + player.name + ": " +  msg.substr(5, 50));
					}
					else{
						room.sendChat("ê°œì¸ : [" + p.getIdofPlayer(player) + "]" + p.checkMarks(player) + player.name + "â†’ [" + p.getIdofPlayer(p.members[i]) + "]" + p.checkMarks(p.members[i]) + p.members[i].name + ": " + msg.substr(5, 50), player.id);
						room.sendChat("ê°œì¸ : [" + p.getIdofPlayer(player) + "]" + p.checkMarks(player) + player.name + "â†’ [" + p.getIdofPlayer(p.members[i]) + "]" + p.checkMarks(p.members[i]) + p.members[i].name + ": " +  msg.substr(5, 50) + " (ë‹µì¥:  !e " + p.getIdofPlayer(player) + " ë‹µí•  ë‚´ìš©)", p.members[i].id);
						cout(true, "ê°œì¸ : [" + p.getIdofPlayer(player) + "]" + p.checkMarks(player) + player.name + "â†’ [" + p.getIdofPlayer(p.members[i]) + "]" + p.checkMarks(p.members[i]) + p.members[i].name + ": " + msg.substr(5, 50));
					}
				}
			}
			// ê·“ì†ë§ ì²˜ë¦¬
			return false;
		}
		this.teamChat = function(player, msg)							// !t 					|	íŒ€ ì±„íŒ… ëª…ë ¹ì–´
		{
			p.members = room.getPlayerList();
			var toTeam;
			
			switch(player.team){
				case 0: toTeam = "ê´€ì¤‘"; break;
				case 1: toTeam = "ë ˆë“œ"; break;
				case 2: toTeam = "ë¸”ë£¨"; break;
			}
			room.sendChat(toTeam + " : [" + p.getIdofPlayer(player) + "]" + p.checkMarks(player) + player.name + ": " + msg.substr(3, 50), player.id);
			
			for(var j = 1; j <= p.cntPlayers; j++){
				// í”Œë ˆì´ì–´ë¥¼ ì œì™¸í•œ ë‚˜ë¨¸ì§€ íŒ€ì› ì°¾ê¸°
				if(p.getIdofPlayer(p.members[j]) != player.id){
					if(p.getTeamofPlayer(p.members[j]) == player.team)
					room.sendChat(toTeam + " : [" + p.getIdofPlayer(player) + "]" + p.checkMarks(player) + player.name + ": " +  msg.substr(3, 50) + " (íŒ€ ì±„íŒ…: !t ë‹µí•  ë‚´ìš©)", p.members[j].id);
				}
			}
			return false;
		}
		this.allChat = function(player, msg){							// !a 					|	ì „ì²´ ì±„íŒ… ëª…ë ¹ì–´
			let senseFilter = msg.search('!a');
			var i = 0;
			
			if(senseFilter != -1) i = 3;	
			room.sendChat(" ì „ì²´ : [" + p.getIdofPlayer(player) + "]" + p.checkMarks(player) + player.name + ": " + msg.substr(i, 50));
			return false;
		}
		
	}
}
class Commands{														// ëª…ë ¹ì–´ í´ë˜ìŠ¤
	constructor(){
		this.plaster = function(player){										// !ë„			|	ë„ì›€ë§, ë„ë°°ë°©ì§€ë¬¸ì(onlyadmin)
    		if (player.admin || admin.subAdmin[player.id - 1]){ 
				room.sendChat(chat.getFace(chat.face[9], 'AlphaGo'));
				room.sendChat(chat.getFace(chat.face[9], 'AlphaGo') + ': ' + n.cnot() + 'ë„ë°° ë°©ì§€');
				room.sendChat(chat.getFace(chat.face[9], 'AlphaGo') + ': ' + n.cnot() + 'ë¶„ë€ ë°©ì§€');
				room.sendChat(chat.getFace(chat.face[9], 'AlphaGo') + ': ' + n.cnot() + 'ì •ìˆ™ ìœ ì§€');
				room.sendChat(chat.getFace(chat.face[9], 'AlphaGo') + ': ' + n.cnot() + 'ì§ˆì„œ ìœ ì§€');
				room.sendChat(chat.getFace(chat.face[9], 'AlphaGo') + ': ');
				cout(true, player.name + '(ì´)ê°€ ë„ë°° ë°©ì§€ ë¬¸ìë¥¼ ì¶œë ¥í•¨.');
			}
			else com.comHelp(player, '!ë„');	// ë„ì›€ë§
			return false; 						// ëª…ë ¹ì–´ í”ì ì„ ë‚¨ê¸°ì§€ ì•ŠìŒ
		}
		this.comHelp = function(player, msg){ 									// !help		|	ëª…ë ¹ì–´ ë„ì›€ë§
			var players = room.getPlayerList();

			chat.send(player, msg, true);
			for(var i = 1; i <= p.cntPlayers; i++){
				if(players[i].admin || admin.subAdmin[player.id - 1]){
					n.announce(n.msgCommand('ì¼ë°˜ ') 
					+ '!adminhelp(ë°© ê´€ë¦¬) | !bothelp(ë´‡ë°©) | !maphelp(ë§µ) | !join(íˆ¬ì…) | !chathelp(ì±„íŒ…) | !etchelp(ê¸°íƒ€)', players[i].id, null, 4, 3);

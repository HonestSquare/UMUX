function cout(io, msg){          // ë°©ì—ì„œ ì½˜ì†”ì°½ìœ¼ë¡œ ì „ë‹¬
	if(msg){
		if(!io) return console.log(t.showDate() + ' â—€ ' + msg);
		else return console.log(t.showDate() + ' â–¶ ' + msg);
	}
}
function c(msg){                // ì½˜ì†”ì°½ì—ì„œ ë°©ìœ¼ë¡œ ì „ë‹¬
	room.sendChat(' ì™¸ë¶€ : [0]ğŸŒê´€ë¦¬ì: ' + msg + ' (ê·“ì†ë§ ë‹µì¥: !e 0 ë‹µí•  ë‚´ìš©)'); 
	cout(true, 'ì „ë‹¬: ' + msg);
};

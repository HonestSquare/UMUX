class Player{														// í”Œë ˆì´ì–´ í´ë˜ìŠ¤
	constructor() {
		// ì£¼ê¶Œí•œ, ë³´ì¡°ê¶Œí•œ, ì¼ë°˜, ë¸”ë™ë¦¬ìŠ¤íŠ¸
		this.mark = ["ğŸŸ¡", "ğŸŸ¢", "âšª", "âš«"];
		this.blacklist = [ 										// ë¸”ë™ë¦¬ìŠ¤íŠ¸ ëª…ë‹¨
                      // **ì—¬ê¸°ì— ë¸”ë™ë¦¬ìŠ¤íŠ¸ ëª…ë‹¨ì„ ê¸°ì…í•˜ì„¸ìš”.**
		];
		this.cntPlayers = 0;									// í”Œë ˆì´ì–´ ì¸ì› ì²´í¬
		this.members;
		this.playersid = new Array(15);							// í”Œë ˆì´ì–´ ID

		this.onPlayerTeamChange = function(player){				// íŒ€ êµì²´
			return false;
		}
		this.onPlayerAcivity = function(){
			return false;
		}
		this.getIdofPlayer = function(player) {					// í”Œë ˆì´ì–´ ID í™•ì¸
			var players = room.getPlayerList();
			var ids = new Array(15);
			ids = p.getNumbersFun();
			for(var i = 1; i <= 15; i++) {
				if((player.id) && (players[i].id == player.id))
					return ids[i - 1];
			}
			ids = null;
		};
		this.getNumbersFun = function(){						// í”Œë ˆì´ì–´ ë²ˆí˜¸ ê°€ì ¸ì˜¤ê¸°
			var players = room.getPlayerList();
			for(var i = 1; i <= 15; i++){
				if(players[i]) p.playersid[i - 1] = i;
			}
			return p.playersid;
		}
		this.getMarks = function(pub, player, fmsg, lmsg){		// í”Œë ˆì´ì–´ ë§ˆí¬ ë¶€ì—¬
			cout(true, (fmsg + p.checkMarks(player) + lmsg));
			if(pub) return room.sendChat(fmsg + p.checkMarks(player) + lmsg);
			else return room.sendChat(fmsg + p.checkMarks(player) + lmsg, player.id);
			
		}
		this.getTeamofPlayer = function (player) {				// í”Œë ˆì´ì–´ ì†Œì† í™•ì¸
			var players = room.getPlayerList();
			for (var i = 1; i <= 15; i++) {
				if (players[i].id == player.id)
					return player.team;
			}
			ofTeam = null;
		};
		this.updatePlayers = function(player) {	// í”Œë ˆì´ì–´ ì •ë³´ ê°±ì‹ 
			var players = room.getPlayerList();
			//var tempop;
			var tempPlayers;
			var tempid = player.id;
			//chatmode[p.cntPlayers] = null;
			if(admin.subAdmin[player.id]) admin.getSubAdmin(players[1]);
			// í•œ ì¹¸ì”© ì±„ìš°ê¸°
			for (var i = tempid; i <= p.cntPlayers; i++) {
				tempPlayers = players[i + 1];
				//tempop = chatmode[i];
				players[i + 1] = null;
				//chatmode[i] = null;
				players[i] = tempPlayers;
				//chatmode[i-1] = tempop;
			}
			return true;
		};
		this.checkMarks = function(player){						// í”Œë ˆì´ì–´ ë§ˆí¬ ì •ë³´
			if(player.admin) return p.mark[0];							// ì£¼ê¶Œí•œ
			else if(admin.subAdmin[player.id - 1]) return p.mark[1];	// ë³´ì¡°ê¶Œí•œ
			else if(admin.filterPlayer(player)) return p.mark[3];			// ë¸”ë™ë¦¬ìŠ¤íŠ¸
			else return p.mark[2];										// ì¼ë°˜
		}
		this.updateMarks = function(){							// í”Œë ˆì´ì–´ ë§ˆí¬ ê°±ì‹ 
			var players = room.getPlayerList();
			for(var i = 0; i < p.cntPlayers; i++){
				p.checkMarks(players[i]);
			}
			return true;
		}
		this.setAvatar = function(player, msg){					// ë“±ë²ˆí˜¸ ì„¤ì •
			if(msg.substr(1 ,6) == "avatar") 
				room.setPlayerAvatar(player.id, msg.substr(8, 10));
			else room.setPlayerAvatar(player.id, msg.substr(5, 10));
			return false;
		}
		this.getX = function(player){							// í”Œë ˆì´ì–´ Xì¢Œí‘œ
			return player.position().x;
		}
		this.getY = function(player){							// í”Œë ˆì´ì–´ Yì¢Œí‘œ
			return player.position().y;
		}
		this.getPos = function(player){							// í”Œë ˆì´ì–´ ì¢Œí‘œ
			return player.position();
		}
	}
}
class TimeManager{													// ì‹œê°„ ê´€ë¦¬ í´ë˜ìŠ¤
	constructor(){
		this.y; this.m; this.d;			// ì—°, ì›”, ì¼
		this.h; this.min; this.sec		// ì‹œ, ë¶„, ì´ˆ
		this.co = '/'; this.ti = ':';
		//-----------------------ë‚ ì§œ ë° ì‹œê°„----------------------------------------------
		this.getDate = function(){ 								// ë‚ ì§œ ë° ì‹œê°„ ë°˜í™˜
			return t.updateDate();
		}
		this.updateTime = function(){							// ë‚ ì§œ ë° ì‹œê°„ ê°±ì‹ 
			date = new Date();
			t.y = date.getFullYear(); t.m = (date.getMonth() + 1); t.d = date.getDate();
			t.h = date.getHours(); t.min = date.getMinutes(); t.sec = date.getSeconds();
			return date;
		}
		this.showDate = function(){ 							// ë‚ ì§œ ë° ì‹œê°„ ì¶œë ¥
			t.getDate();
			return (t.showTimsg() + ' ' + t.showTime());
		}
		//-----------------------ë‚ ì§œ----------------------------------------------------
		this.getTime = function(){ 							// ë‚ ì§œ ë°˜í™˜
			return t.updateTime();
		}
		this.updateDate = function(){							// ë‚ ì§œ ê°±ì‹ 
			date = new Date();
			t.y = date.getFullYear(); t.m = (date.getMonth() + 1); t.d = date.getDate();
			return date;
		}		
		this.showTime = function(){ 							// ë‚ ì§œ ì¶œë ¥
			t.getTime(); 
			return (t.y + t.co + t.m + t.co + t.d);
		}	
		//-----------------------ì‹œê°„----------------------------------------------------
		this.getTime = function(){ 								// ì‹œê°„ ë°˜í™˜
			return t.updateTime();
		}
		this.updateTime = function(){							// ì‹œê°„ ê°±ì‹ 
			date = new Date();
			t.h = date.getHours(); t.min = date.getMinutes(); t.sec = date.getSeconds();
			// ì´ˆ ë‹¨ìœ„ë¡œ ë°˜í™˜
			return ((t.h * 3600) + (t.min * 60) + t.sec);
		}	
		this.showTime = function(){ 							// ì‹œê°„ ì¶œë ¥
			t.getTime(); 
			return (t.h + t.ti + t.min + t.ti + t.sec);
		}
		
	}
}


		}
		this.setJoinSpec = function(player){ return com.setJoinPlayer(player.team, 0, player.name, player.id);}		// 0: ê´€ì¤‘
		this.setJoinRed = function(player){ return com.setJoinPlayer(player.team, 1, player.name, player.id);}		// 1: ë ˆë“œ
		this.setJoinBlue = function(player){ return com.setJoinPlayer(player.team, 2, player.name, player.id);}		// 2: ë¸”ë£¨		
		this.loadMap = function(player, mapId){									// !load n		|	ìœ ì¦ˆë§µ ë¡œë“œ ëª…ë ¹ì–´(onlyadmin)
			if (player.admin || admin.subAdmin[player.id - 1]){
				if((mapId.substr(6, 2)) >= 1 && ((mapId.substr(6, 2)) <= 12)){
					room.stopGame();
					room.setCustomStadium(map[mapId.substr(6, 2) - 1]);
					cout(true, player.name + '(ì´)ê°€ ' + mapId.substr(6, 2) + 'ë²ˆ ë§µìœ¼ë¡œ êµì²´í•¨.');
				}
				else{ 
					n.announce(n.cnot() + "1~12 ì•ˆì˜ ë²”ìœ„ë¥¼ ì…ë ¥í•˜ì„¸ìš”.", player.id, 'orange', 5, 1);
					cout(true, player.name + '(ì´)ê°€ ë§µ êµì²´ë¥¼ ì‹œë„í•¨ (ì‹¤íŒ¨).');
				}
			}
			else n.acess(player);
			return false;
		}
		// ------------ì´ìŠ¤í„° ì—ê·¸------------------
		this.joke = function(player, msg){										//	!ë„ë‹´		|	joke ëª…ë ¹ì–´

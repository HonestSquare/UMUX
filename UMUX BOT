room.onPlayerJoin = function(player) { return game.onPlayerJoin(player); }			// í”Œë ˆì´ì–´ ì…ì¥
room.onPlayerLeave = function(player) { return game.onPlayerLeave(player); }		// í”Œë ˆì´ì–´ í‡´ì¥
room.onPlayerAcivity = function(){ return p.onPlayerAcivity() }
// í”Œë ˆì´ì–´ ê°•ì œ í‡´ì¥
room.onPlayerKicked = function(kickedPlayer, reason, ban, byPlayer){ return admin.onPlayerKicked(kickedPlayer, reason, ban, byPlayer) }
room.onPlayerAdminChange = function(player){ return admin.onPlayerAdminChange(player) }		// í”Œë ˆì´ì–´ ê¶Œí•œ íšë“&ë°•íƒˆ
room.onStadiumChange = function(mapName){ return game.onStadiumChange(mapName); }			// ë§µ êµì²´
// ì±„íŒ… ì‹œìŠ¤í…œ
room.onPlayerChat = function(player, msg) { return chat.onPlayerChat(player, msg); }

room.onPositionsReset = function(){ return game.onPositionsReset(); }				// ì¢Œí‘œ ì´ˆê¸°í™”
room.onPlayerBallKick = function(){ return game.onPlayerBallKick(); }				// ê³µ ì°¼ì„ ë•Œ
room.onTeamGoal = function(team){ return game.onTeamGoal(team); }					// ê³¨ ë¨¹í ë•Œ
room.onPlayerTeamChange = function(player){ return p.onPlayerTeamChange(player) }	// íŒ€ êµì²´
room.onTeamVictory = function(scores){ return game.onTeamVictory(scores); }			// íŒ€ ìŠ¹ë¦¬
room.onGameStart = function(){ return game.onGameStart(); }							// ê²Œì„ ì‹œì‘
room.onGameTick = function(){ return game.onGameTick(); }							// ê²Œì„ ë„ì¤‘
room.onGameStop = function(){ return game.onGameStop(); }							// ê²Œì„ ì¢…ë£Œ
room.onGamePause = function(byPlayer) { return game.onGamePause(byPlayer);}			// ê²Œì„ ì¤‘ë‹¨
room.onGameUnpause = function(byPlayer) { return game.onGameUnpause(byPlayer); }	// ê²Œì„ ì¬ê°œ
